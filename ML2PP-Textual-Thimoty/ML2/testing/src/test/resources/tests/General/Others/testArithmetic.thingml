import "../../thingml.thingml"

thing TestArithmetic includes Test 
@test "x # x"
{
  			property x : Integer = 2
    statechart TestPropertyPing init INIT {
    	state INIT {
    		transition -> STOP
			event m : Test?In
			guard m.c == 'x' and (3 + x) * ((6 - 4) * 2) == 20
			action do
			    TestOut('x')
            end			  		

    		transition -> STOP
			event m : Test?In
			guard m.c == 'x' and not((3 + x) * ((6 - 4) * 2) == 20)
			action do
			    TestOut('z')
            end
    	}

		state STOP {
			on entry TestDone()
		}
    }
}
