import "../../thingml.thingml"

thing TestVariables includes Test 
@test "x # abcde"
{
	property p : Char = 'a'
	property arr : Char[1] = {'c'}
    
    statechart TestPropertyPing init state0 {
    	
    	on entry TestOut(p)
    	
    	state state0 {
    		
    		property p : Char = 'b'
    		
    		on entry TestOut(p)
    		
			transition -> state1 
			event e : Test?In
			guard e.c == 'x'
			action do
				var p : Char = 'e'
				do
					var p : Char = 'd'
					for(p : Char in arr) do
						TestOut(p)
					end
					TestOut(p)
				end
				TestOut(p)
			end
		}

    	state state1 {
    		internal event Test?Done
			action TestDone()	
		}
    }
}
