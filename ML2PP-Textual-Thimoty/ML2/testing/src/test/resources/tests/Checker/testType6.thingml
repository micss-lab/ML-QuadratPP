datatype DT<1>
@type_checker "Boolean";

thing TestCheckStateMachine
{       
	function f() do end
	
    statechart TestAbstractFunction init INIT {
    	state INIT {  
    		on entry do
    			var p : DT
    			p = f() //should fail as we cannot assign a var to Void
    		end  		    
    	}
    }
}
