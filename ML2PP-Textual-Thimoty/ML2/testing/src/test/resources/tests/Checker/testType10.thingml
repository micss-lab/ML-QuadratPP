datatype Int<2>
@type_checker "Integer";

datatype Byte<1>
@type_checker "Byte";

thing TestCheckStateMachine
@checker_should_fail "false"
{       
	function f() do end
	
    statechart TestAbstractFunction init INIT {
    	state INIT {  
    		on entry do    			
    			var p : Int
    			p = 0xFF //should not fail as an Integer can be assigned from a Byte
    		end  		    
    	}
    }
}
