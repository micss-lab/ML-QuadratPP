import "datatypes.thingml" from stl

thing TestCheckFor
@checker_should_fail "false"
{
	property arr : Integer[3] = {1, 2, 3}
	
    statechart TestAbstractFunction init INIT {
    	state INIT {
    		on entry do
    			for(v : Integer, i : Integer in arr) do //ok
    				println v, i
    			end
    		end
    	}
    }
}