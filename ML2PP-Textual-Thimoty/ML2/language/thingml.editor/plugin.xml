<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated ThingML -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.thingml.xtext.thingML.presentation.ThingMLModelWizardID"
            name="%_UI_ThingMLModelWizard_label"
            class="org.thingml.xtext.thingML.presentation.ThingMLModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ThingMLModelFile.gif">
         <description>%_UI_ThingMLModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated ThingML -->
      <editor
            id="org.thingml.xtext.thingML.presentation.ThingMLEditorID"
            name="%_UI_ThingMLEditor_label"
            icon="icons/full/obj16/ThingMLModelFile.gif"
            extensions="thingml"
            class="org.thingml.xtext.thingML.presentation.ThingMLEditor"
            contributorClass="org.thingml.xtext.thingML.presentation.ThingMLActionBarContributor">
      </editor>
   </extension>

</plugin>
